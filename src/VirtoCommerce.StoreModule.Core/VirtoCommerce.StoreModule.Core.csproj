<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <noWarn>1591</noWarn>
        <PublishRepositoryUrl>true</PublishRepositoryUrl>
        <EmbedUntrackedSources>true</EmbedUntrackedSources>
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    </PropertyGroup>
    <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
        <NoWarn>1701;1702;1705;1591</NoWarn>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
        <PackageReference Include="VirtoCommerce.Platform.Core" Version="3.78.0" />
        <PackageReference Include="VirtoCommerce.CoreModule.Core" Version="3.1.0" />
        <PackageReference Include="VirtoCommerce.NotificationsModule.Core" Version="3.14.0" />
        <PackageReference Include="VirtoCommerce.Platform.Core" Version="3.78.0" />
    </ItemGroup>
    <ItemGroup>
      <None Remove="VirtoCommerce.Platform.Core" />
    </ItemGroup>
</Project>